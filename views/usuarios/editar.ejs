        <div class="main-panel">
            <div class="content-wrapper">
                <h2 class="mb-4">Editar Usuario</h2>
                <div class="row justify-content-center">
                    <div class="col-12 grid-margin">
                        <form class="form-sample" action="/usuarios/editar" method="POST">
                            <div class="card mb-5">
                                <div class="card-body">
                                <h4 class="card-title text-uppercase">Editar datos de la cuenta</h4>
                                    <div class="row">
                                        <div class="col-12 col-lg-6">
                                            <div class="form-group row " id="privilegioRegisterChange">
                                                <label class="col-sm-3 col-form-label">Tipo de usuario</label>
                                                <div class="col-sm-8">
                                                    <div class="form-radio">
                                                        <label class="form-check-label">
                                                            <select class="form-control" id="privilegioSelect" name="privilege">
                                                                <% if (user.privilege == 'Usuario'){ %>
                                                                    <option disabled>Seleccionar tipo de usuario</option>
                                                                    <option value="Usuario" selected>Usuario</option>
                                                                    <option value="Proveedor">Proveedor</option>
                                                                <% } %>
                                                                <% if (user.privilege == 'Proveedor'){ %>
                                                                    <option disabled>Seleccionar tipo de usuario</option>
                                                                    <option value="Usuario" >Usuario</option>
                                                                    <option value="Proveedor" selected>Proveedor</option>
                                                                <% } %>
                                                            </select>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group row">
                                                <label class="col-sm-3 col-form-label">Usuario</label>
                                                <div class="col-sm-9">
                                                    <input type="text" class="form-control" value="<%= user.username %>" name="username" />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-sm-3 col-form-label">Correo Electrónico</label>
                                                <div class="col-sm-9">
                                                    <input type="email" name="email" value="<%= user.email %>" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group row">
                                                <label class="col-sm-3 col-form-label">Contraseña</label>
                                                <div class="col-sm-9">
                                                    <input type="password" name="password" class="form-control" />
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-sm-3 col-form-label">Repetir contraseña</label>
                                                <div class="col-sm-9">
                                                    <input type="password" class="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card" id="usuario">
                                <div class="card-body">
                                    <h4 class="card-title text-uppercase">Información del cliente</h4>
                                    <p class="card-description font-weight-bold">
                                        Información personal
                                    </p>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group row">
                                            <label class="col-sm-3 col-form-label">Nombres y Apellidos</label>
                                            <div class="col-sm-9">
                                                <input type="text" name="nombreCompleto" value="<%= profile.nombreCompleto %>" class="form-control" />
                                            </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group row">
                                                <label class="col-sm-3 col-form-label">Genero</label>
                                                <div class="col-sm-9">
                                                    <select name="genero" class="form-control">
                                                        <% if(profile.genero == 'Masculino'){ %>
                                                            <option value="Masculino" selected>Masculino</option>
                                                            <option value="Femenino">Femenino</option>
                                                        <% } %>
